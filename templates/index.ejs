                <div class="container">
                    <div class="row">
                        <div class="clearfix skill-inner">
                            <div class="col-sm-12 col-md-3">
                                <div class="skill-left">
                                    <h2 class="title">Skills</h2>
                                    <p class="regular-text">«Wisdom is knowing how little we know.» <br/> Oscar Wilde
                                    </p>
                                </div>
                            </div>

                            <!-- skills container -->
                            <div class="col-sm-12 col-md-6 col-md-offset-1">
                                <div class="skill-right">
                                    <ul id="skillSlider" class="clearfix skill-graph">

                                        <% skills.forEach(function(skill){ %>
                                        <!-- single skill -->
                                        <li>
                                            <div class="after-li">
                                                <div class="singel-hr">
                                                    <div class="singel-hr-inner" data-height="<%-skill.level%>">
                                                        <div class="skill-visiable">
                                                            <span class="skill-title"><%-skill.name%></span>
                                                            <div class="hr-wrap">
                                                                <div class="hrc"></div>
                                                            </div>
                                                        </div>
                                                    </div>
                                                    <span class="skill-count"><%-skill.level%></span>
                                                </div>
                                            </div>
                                        </li>
                                        <!-- single skill -->
                                        <% }); %>

                                    </ul>
                                </div>
                            </div>
                            <!-- /skills container -->
                        </div>
                    </div>
                    <div class="btn-wrapp skl-ctrl">
                        <a class="btn-floating waves-effect waves-light btn-large cyan white-text go go-left"><i
                                class="mdi-navigation-chevron-left "></i></a>
                        <a class="btn-floating waves-effect waves-light btn-large cyan white-text go go-right"><i
                                class="mdi-navigation-chevron-right "></i></a>
                    </div>
                </div>
            </div>
            <!-- .container end -->
        </section>

        <section id="experience" class="root-sec padd-tb-120 cyan experience-wrap">
            <div class="container">
                <div class="row">
                    <div class="experience-inner">
                        <div class="col-sm-12 col-md-10 card-box-wrap">
                            <div class="row">
                                <div class="clearfix section-head experience-text">
                                    <div class="col-sm-12">
                                        <h2 class="title">Experience</h2>
                                        <p class="regular-text">«Experience is simply the name we give our mistakes.»
                                            <br/> Oscar Wilde </p>
                                    </div>
                                </div>
                                <div class="col-lg-12">
                                    <div class="overflow-hidden">
                                        <div class="row">
                                            <div id="experienceSlider" class="clearfix card-element-wrapper">


                                                <% works.forEach(function(work){ %>
                                                <!-- single work -->

                                                <div class="col-sm-4 cold-xs-12 single-card-box wow fadeInUpSmall"
                                                     data-wow-duration=".7s">
                                                    <div class="card">
                                                        <div class="card-image waves-effect waves-block waves-light">
                                                            <h2 class="left-align card-title-top">Period:
                                                                <%-work.period%></h2>
                                                            <div class="valign-wrapper card-img-wrap">
                                                                <img class="activator" loading="lazy" src="<%-work.logo%>" alt="<%-work.name%>">
                                                            </div>
                                                        </div>
                                                        <div class="card-content">
                                                                <span class="card-title activator cyan-text"><%-work.position%><i
                                                                        class="mdi-navigation-more-vert right"></i></span>
                                                            <p><%-work.name%>, <%-work.type%></p>
                                                        </div>
                                                        <div class="card-reveal">
                                                            <div class="rev-title-wrap">
                                                                    <span class="card-title activator cyan-text"><%-work.position%><i
                                                                            class="mdi-navigation-close right"></i></span>
                                                                <p><%-work.name%>, <%-work.type%></p>
                                                            </div>
                                                            <p class="rev-content">
                                                                <%-work.main_specialization%>
                                                                <br/>
                                                                <%-work.primary_functions%>
                                                                <br/><br/>
                                                                Projects:
                                                                <% work.projects.forEach(function(project){ %>
                                                                <br/>
                                                                <%-project.name%>
                                                                <% }); %>
                                                            </p>
                                                        </div>
                                                    </div>
                                                </div>

                                                <!-- single work -->
                                                <% }); %>

                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="btn-wrapp exp-ctrl">
                            <a class="btn-floating waves-effect waves-light btn-large white go go-left"><i
                                    class="mdi-navigation-chevron-left cyan-text"></i></a>
                            <a class="btn-floating waves-effect waves-light btn-large white go go-right"><i
                                    class="mdi-navigation-chevron-right cyan-text"></i></a>
                        </div>
                    </div>
                </div>
            </div>
        </section>

        <section id="education" class="root-sec grey lighten-5 padd-tb-120 education-wrap">
            <div class="container">
                <div class="row">
                    <div class="education-inner">
                        <div class="col-sm-12 col-md-10 card-box-wrap">
                            <div class="row">
                                <div class="clearfix section-head education-text">
                                    <div class="col-sm-12">
                                        <h2 class="title">Education</h2>
                                        <p class="regular-text">«Formal education will make you a living. Self-education
